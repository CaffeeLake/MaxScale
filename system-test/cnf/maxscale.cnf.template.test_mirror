[maxscale]
threads=###threads###
log_info=1

[MySQL-Monitor]
type=monitor
module=mysqlmon
servers=###server_line###
user=maxskysql
password=skysql
monitor_interval=1000

[Mirror-Router]
type=service
router=mirror
targets=###server_line###
user=maxskysql
password=skysql
main=server1
exporter=file
file=/tmp/mirror.txt

[Mirror-Listener]
type=listener
service=Mirror-Router
protocol=MySQLClient
port=4006

###server###
